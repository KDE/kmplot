                    </property>
                    <property name="whatsThis" stdset="0">
                        <string>Enter the initial y-value or expression for the integral, for example 2 or pi/2</string>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QFrame">
            <property name="name">
                <cstring>frame5_2_2</cstring>
