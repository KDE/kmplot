    <tabstop>showIntegral</tabstop>
    <tabstop>txtInitX</tabstop>
    <tabstop>txtInitY</tabstop>
    <tabstop>customPrecision</tabstop>
    <tabstop>precision</tabstop>
    <tabstop>lineWidth</tabstop>
    <tabstop>color</tabstop>
</tabstops>
